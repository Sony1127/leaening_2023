#include <stdio.h>

int compareBoxes(int boxA[], int boxB[], int size) {
    int count[size];  // Array to keep track of item counts in boxB

    // Initialize count array with zeros
    for (int i = 0; i < size; i++) {
        count[i] = 0;
    }

    // Iterate over boxA and boxB to count the occurrences of items in boxB
    for (int i = 0; i < size; i++) {
        int itemA = boxA[i];
        int itemB = boxB[i];
        int found = 0;  // Flag to check if itemB is found in boxB

        // Search for itemB in boxB
        for (int j = 0; j < size; j++) {
            if (itemB == boxB[j] && count[j] == 0) {
                count[j] = 1;  // Mark the item as found in boxB
                found = 1;     // Set the flag to true
                break;
            }
        }

        // If itemB is not found in boxB, return 0
        if (found == 0) {
            return 0;
        }
    }

    // Check if all items in boxB are found in boxA
    for (int i = 0; i < size; i++) {
        if (count[i] == 0) {
            return 0;
        }
    }

    // If all items are found in boxA and boxB, return 1
    return 1;
}

int main() {
    int boxA[] = {200, 10, -90};  // First box
    int boxB[] = {-90, 200, 10};  // Second box
    int size = sizeof(boxA) / sizeof(boxA[0]);

    int result = compareBoxes(boxA, boxB, size);

    printf("Result: %d\n", result);

    return 0;
}