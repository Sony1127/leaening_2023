#include <stdio.h>
#include <ctype.h>

int findCharType(char ch) {
    if (isalpha(ch)) {
        if (isupper(ch)) {
            return 1; // 'A' to 'Z'
        } else {
            return 2; // 'a' to 'z'
        }
    } else if (isdigit(ch)) {
        return 3; // '0' to '9'
    } else if (ispunct(ch)) {
        if (isprint(ch)) {
            return 4; // Any other printable symbols
        } else {
            return 5; // Non-printable symbols
        }
    } else {
        return 0; // Invalid character
    }
}

int main() {
    char ch;
    
    printf("Enter a character: ");
    scanf(" %c", &ch);
    
    int result = findCharType(ch);
    
    switch (result) {
        case 1:
            printf("Type: 'A' to 'Z'\n");
            break;
        case 2:
            printf("Type: 'a' to 'z'\n");
            break;
        case 3:
            printf("Type: '0' to '9'\n");
            break;
        case 4:
            printf("Type: Any other printable symbols\n");
            break;
        case 5:
            printf("Type: Non-printable symbols\n");
            break;
        default:
            printf("Invalid character\n");
            break;
    }
    
    return 0;
}
