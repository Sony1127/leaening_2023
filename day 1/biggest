#include<stdio.h>
int main(void)
{
    int a,b,max;
 
    // Take input from the user
    printf("Enter values for a and b : ");
    scanf("%d%d", &a, &b);
 
    // Use the ternary Operator to calculate the Maximum
    max = a>b ? a : b;
    printf("Larger of %d and %d is %d \n", a, b, max);
    return 0;
}